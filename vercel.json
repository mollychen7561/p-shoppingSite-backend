{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/api/index.ts"
    }
  ],
  "functions": {
    "src/api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
